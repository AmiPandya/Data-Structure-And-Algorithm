
class Solution {
    public int[] solution(int[] A, int k) {
        if(k < A.length){
            k = k% A.length;
        }
        int copy[] = new int[A.length];
        int j =0;
        for(int i= 0 ; i < A.length; i++)
        {
        j = (i +k) % A.length;
        copy[j]= A[i];
    }
    return copy;
    
}
}
